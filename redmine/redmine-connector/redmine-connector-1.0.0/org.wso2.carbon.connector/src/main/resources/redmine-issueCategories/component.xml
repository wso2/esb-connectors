<component name="redmine-issueCategories" type="synapse/template">
  <subComponents>
    <component name="getIssueCategories">
      <file>getIssueCategories.xml</file>
      <description>Returns a list of issue categories.</description>
    </component>
      <component name="getIssueCategoryById">
          <file>getIssueCategoryById.xml</file>
          <description>Returns a particular issue category.</description>
      </component>
      <component name="createIssueCategory">
          <file>createIssueCategory.xml</file>
          <description>Creatss a new issue category.</description>
      </component>
      <component name="updateIssueCategory">
          <file>updateIssueCategory.xml</file>
          <description>Updates an issue category.</description>
      </component>
      <component name="deleteIssueCategory">
          <file>deleteIssueCategory.xml</file>
          <description>Deletes an issue category.</description>
      </component>
  </subComponents>
</component>
<component name="redmine-projectMemberships" type="synapse/template">
  <subComponents>
    <component name="getProjectMemberships">
      <file>getProjectMemberships.xml</file>
      <description>Gets memberships of a single project by its ID. </description>
    </component>
      <component name="getMembershipByMembershipId">
          <file>getMembershipByMembershipId.xml</file>
          <description>Gets memberships of a given membership id.</description>
      </component>
    <component name="addProjectMember">
      <file>addProjectMember.xml</file>
      <description>Adds a new member to a project.</description>
    </component>
      <component name="deleteMemberships">
          <file>deleteMemberships.xml</file>
          <description>Deletes the memberships of a user.</description>
      </component>
    <component name="updateRolesOfAMembership">
          <file>updateRolesOfAMembership.xml</file>
          <description>Updates the roles of a membership.</description>
      </component>
  </subComponents>
</component>
<?xml version="1.0" encoding="UTF-8"?>
<proxy  xmlns="http://ws.apache.org/ns/synapse" name="ejb2" startOnLoad="true" trace="disable" transports="https http" >
	<description/>
	<target>
		<inSequence>
			<ejbconnector.init>
				<key>raj</key>
				<raj.java.naming.factory.initial>com.sun.enterprise.naming.SerialInitContextFactory</raj.java.naming.factory.initial>
				<raj.org.omg.CORBA.ORBInitialHost>localhost</raj.org.omg.CORBA.ORBInitialHost>
				<raj.org.omg.CORBA.ORBInitialPort>3700</raj.org.omg.CORBA.ORBInitialPort>
				<raj.java.naming.factory.state>com.sun.corba.ee.impl.presentation.rmi.JNDIStateFactoryImpl</raj.java.naming.factory.state>
				<raj.java.naming.factory.url.pkgs>com.sun.enterprise.naming</raj.java.naming.factory.url.pkgs>
			</ejbconnector.init>
			<ejbconnector.stateful>
				<jndiName>HelloStateful</jndiName>
				<method>setName</method>
				<context>raj</context>
				<key>jack4</key>
				<jack4.arg1>Rajjaz</jack4.arg1>
			</ejbconnector.stateful>
			<log level="full">
				<property expression="get-property('Response')" name="out"/>
			</log>
			<ejbconnector.stateful>
				<jndiName>HelloStateful</jndiName>
				<method>getName</method>
				<context>raj</context>
				<key>jack5</key>
			</ejbconnector.stateful>
			<log level="full">
				<property expression="get-property('Response')" name="out"/>
			</log>
		</inSequence>
	</target>
</proxy>
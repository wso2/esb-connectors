<component name="teamwork_taskLists" type="synapse/template">
    <subComponents>
        <component name="createTaskList">
            <file>createTaskList.xml</file>
            <description>Create a single task list.</description>
        </component>
        <component name="updateTaskList">
            <file>updateTaskList.xml</file>
            <description>Modifies a single task list.</description>
        </component>
        <component name="deleteTaskList">
            <file>deleteTaskList.xml</file>
            <description>Delete a task list.</description>
        </component>
        <component name="getTaskList">
            <file>getTaskList.xml</file>
            <description>Returns a single task list identified by its ID.</description>
        </component>
        <component name="getAllTemplateTaskLists">
            <file>getAllTemplateTaskLists.xml</file>
            <description>Retrieves all template task lists.</description>
        </component>
        <component name="getTaskListsOnProject">
            <file>getTaskListsOnProject.xml</file>
            <description>Retrieve all the task lists on a project.</description>
        </component>
        <component name="reorderTaskLists">
            <file>reorderTaskLists.xml</file>
            <description>Reorder the task lists.</description>
        </component>
    </subComponents>
</component>
<component name="teamwork_timeTracking" type="synapse/template">
    <subComponents>
        <component name="updateTimeEntry">
            <file>updateTimeEntry.xml</file>
            <description>Modifies a single time entry.</description>
        </component>
        <component name="deleteTimeEntry">
            <file>deleteTimeEntry.xml</file>
            <description>Delete a time entry.</description>
        </component>
        <component name="getTimeEntry">
            <file>getTimeEntry.xml</file>
            <description>Returns a single time entry identified by its ID.</description>
        </component>
        <component name="getAllTimeEntries">
            <file>getAllTimeEntries.xml</file>
            <description>Retrieves all the time entries.</description>
        </component>
        <component name="getTimeEntriesForProject">
            <file>getTimeEntriesForProject.xml</file>
            <description>Retrieves all the time entries for a project.</description>
        </component>
        <component name="getTimeEntriesForToDoItem">
            <file>getTimeEntriesForToDoItem.xml</file>
            <description>Retrieves all the time entries for a todo item.</description>
        </component>
        <component name="getTimeTotalsOnProject">
            <file>getTimeTotalsOnProject.xml</file>
            <description>Retrieves all the time totals for a project.</description>
        </component>
        <component name="getTimeTotalsOnTask">
            <file>getTimeTotalsOnTask.xml</file>
            <description>Retrieves all the time totals for a todo item.</description>
        </component>
        <component name="getTimeTotalsOnTaskList">
            <file>getTimeTotalsOnTaskList.xml</file>
            <description>Retrieves all the time totals for a task list.</description>
        </component>
    </subComponents>
</component>
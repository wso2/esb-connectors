<component name="teamwork_notebooks" type="synapse/template">
    <subComponents>
        <component name="updateNotebook">
            <file>updateNotebook.xml</file>
            <description>Modifies a single Notebook.</description>
        </component>
        <component name="deleteNotebook">
            <file>deleteNotebook.xml</file>
            <description>Delete a Notebook.</description>
        </component>
        <component name="getNotebook">
            <file>getNotebook.xml</file>
            <description>Returns a single Notebook identified by its ID.</description>
        </component>
        <component name="getAllNotebooks">
            <file>getAllNotebooks.xml</file>
            <description>Retrieves all accessible Notebooks.</description>
        </component>
        <component name="getNotebooksInCategory">
            <file>getNotebooksInCategory.xml</file>
            <description>Retrieve all the notebooks for a specific category.</description>
        </component>
        <component name="getNotebooksOnProject">
            <file>getNotebooksOnProject.xml</file>
            <description>Retrieve all the notebooks for a project.</description>
        </component>
        <component name="lockNotebook">
            <file>lockNotebook.xml</file>
            <description>Locks the notebook and all versions for editing.</description>
        </component>
        <component name="unlockNotebook">
            <file>unlockNotebook.xml</file>
            <description>Unlocks a locked notebook so it can be edited again.</description>
        </component>
    </subComponents>
</component>
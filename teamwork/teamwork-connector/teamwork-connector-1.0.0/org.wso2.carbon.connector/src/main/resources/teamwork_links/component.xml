<component name="teamwork_links" type="synapse/template">
    <subComponents>
        <component name="updateLink">
            <file>updateLink.xml</file>
            <description>Modifies a single link.</description>
        </component>
        <component name="deleteLink">
            <file>deleteLink.xml</file>
            <description>Delete a link.</description>
        </component>
        <component name="getLink">
            <file>getLink.xml</file>
            <description>Returns a single Link identified by its ID.</description>
        </component>
        <component name="getAllLinks">
            <file>getAllLinks.xml</file>
            <description>Retrieves all Links.</description>
        </component>
        <component name="getLinksOnProject">
            <file>getLinksOnProject.xml</file>
            <description>Retrieve all the links on a project.</description>
        </component>
    </subComponents>
</component>
<component name="feedly_markers" type="synapse/template">
    <subComponents>
        <component name="getListOfUnreadCount">
            <file>getListOfUnreadCount.xml</file>
            <description>List the list of unread counts.</description>
        </component>
        <component name="markArticleAsRead">
            <file>markArticleAsRead.xml</file>
            <description>Mark one or multiple articles as read.</description>
        </component>
        <component name="markArticleAsUnread">
            <file>markArticleAsUnread.xml</file>
            <description>Mark one or multiple articles as read.</description>
        </component>
        <component name="markFeedAsRead">
            <file>markFeedAsRead.xml</file>
            <description>Mark a feed as read.</description>
        </component>
        <component name="markCategoryAsRead">
            <file>markCategoryAsRead.xml</file>
            <description>Mark a category as read.</description>
        </component>
        <component name="undoMarkAsRead">
            <file>undoMarkAsRead.xml</file>
            <description>Undo mark as read.</description>
        </component>
        <component name="markArticleAsSaved">
            <file>markArticleAsSaved.xml</file>
            <description>Mark one or multiple articles as saved.</description>
        </component>
        <component name="markArticlesAsUnsaved">
            <file>markArticlesAsUnsaved.xml</file>
            <description>Mark one or multiple articles as unsaved.</description>
        </component>
        <component name="getLatestReadOperations">
            <file>getLatestReadOperations.xml</file>
            <description>List the latest read operations.</description>
        </component>
        <component name="getLatestTagEntryIds">
            <file>getLatestTagEntryIds.xml</file>
            <description>List the latest tagged entry ids.</description>
        </component>
        <component name="keepArticlesAsUnread">
            <file>keepArticlesAsUnread.xml</file>
            <description>Keep one or multiple articles as unread.</description>
        </component>
    </subComponents>
</component>
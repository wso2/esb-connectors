<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="wso2.connector.googlecontacts">
  <soapenv:Header></soapenv:Header>
  <soapenv:Body>
    <createContact>
      <urn:accessToken>%s(accessToken)</urn:accessToken>
      <urn:userEmail>%s(userEmail)</urn:userEmail>
      <urn:contactId>%s(contactId)</urn:contactId>
      <urn:name>
        <fullName>Update Contact Mandatory</fullName>
      </urn:name>
      <urn:method>updateContact</urn:method>
    </createContact>
  </soapenv:Body>
</soapenv:Envelope>
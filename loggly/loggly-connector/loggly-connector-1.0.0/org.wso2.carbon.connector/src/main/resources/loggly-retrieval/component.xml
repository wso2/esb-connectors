<component name="loggly-retrieval" type="synapse/template">
  <subComponents>
    <component name="createSearchQuery">
      <file>createSearchQuery.xml</file>
      <description>Construct a search Query based on a given set of parameters to filter the log data and retrieve it</description>
    </component>
    <component name="getSearchedEvents">
      <file>getSearchedEvents.xml</file>
      <description>Get filtered logs by providing the Query ID returned by the createSearchQuery</description>
    </component>
    <component name="getSearchFields">
      <file>getSearchFields.xml</file>
      <description>Get the fields by which the Logs can be queried</description>
    </component>
  </subComponents>
</component>
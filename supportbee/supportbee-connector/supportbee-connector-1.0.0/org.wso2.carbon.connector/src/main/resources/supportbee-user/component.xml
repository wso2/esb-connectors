<component name="supportbee-user" type="synapse/template">
   <subComponents>
      <component name="assignUser">
         <file>assignUser.xml</file>
         <description>Assign the ticket identified by the ticket ID to the specified User.</description>
      </component>
      <component name="getUser">
         <file>getUser.xml</file>
         <description>Retrieve details of a user identified by the provided user ID.</description>
      </component>
      <component name="listAgents">
         <file>listAgents.xml</file>
         <description>Retrieve a List of agents from SupportBee account.</description>
      </component>
   </subComponents>
</component>
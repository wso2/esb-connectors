<component name="supportbee-label" type="synapse/template">
   <subComponents>
      <component name="addLabel">
         <file>addLabel.xml</file>
         <description>Add a label to ticket using the provided Parameters.</description>
      </component>
      <component name="listLabels">
         <file>listLabels.xml</file>
         <description>Retrieve a List of labels from SupportBee account.</description>
      </component>
      <component name="removeLabel">
         <file>removeLabel.xml</file>
         <description>Remove the label identified by the provided label ID from the ticket.</description>
      </component>
   </subComponents>
</component>
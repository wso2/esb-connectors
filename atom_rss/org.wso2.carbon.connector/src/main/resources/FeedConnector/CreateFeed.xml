<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://ws.apache.org/ns/synapse" name="CreateFeed">
    <parameter name="HostAddress"/>
    <parameter name="Title"/>
    <parameter name="Content"/>
    <parameter name="Author"/>
    <parameter name="FeedID"/>
    <sequence>
        <log level="full">
            <property name="HostAddress" expression="$func:HostAddress"/>
            <property name="Title" expression="$func:Title"/>
            <property name="Content" expression="$func:Content"/>
            <property name="Author" expression="$func:Author"/>
            <property name="FeedID" expression="$func:FeedID"/>
        </log>
        <class name="org.wso2.carbon.connector.CreateFeed"/>
    </sequence>
</template>

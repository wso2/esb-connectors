<component name="spotify_albums" type="synapse/template">
    <subComponents>
        <component name="getAnAlbumMetadata">
            <file>getAnAlbumMetadata.xml</file>
            <description>Get Spotify catalog information for a single album identified by the given id.</description>
        </component>
        <component name="getSeveralAlbums">
            <file>getSeveralAlbums.xml</file>
